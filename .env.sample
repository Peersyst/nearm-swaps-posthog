# PostHog
POSTHOG_BASE_URL=https://app.posthog.com
POSTHOG_PROJECT_ID=1234
POSTHOG_API_KEY=phc_xxx

# Swap event schema
SWAP_EVENT_NAME=swap
NETWORK_FILTER=mainnet

# Which leg to value ("in" | "out")
VOLUME_SIDE=in
VOLUME_PROP_IN=amount_in
VOLUME_PROP_OUT=amount_out

# Exclusions by event prop properties.account_id
EXCLUDE_ACCOUNT_ID_PATTERNS=
EXCLUDE_ACCOUNT_IDS=

# Internal prices endpoint (queried ONCE)
PRICES_API_URL=

# Batching
BATCH_SIZE=500
MAX_EVENTS=0      # 0 = no limit; otherwise stops after N events (useful for testing)
